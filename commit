#!/bin/bash

# Copyright Guo Yunhe <guoyunhebrave@gmail.com> 2017

# This script will check translation changes before submit.

# Check trunk branch
cd upstream/kf5-trunk/zh_CN
svn commit -m "Crowdin Sync [kf5-trunk] [zh_CN]"

# Check stable branch
cd ../../kf5-stable/zh_CN
svn commit -m "Crowdin Sync [kf5-stable] [zh_CN]"

echo "Done!"

# End of file
